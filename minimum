import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a[] = new int[n];
        int min=0;
        for(int i=0;i<n;i++){
            a[i] = sc.nextInt();
            if(i==0)min=a[0];
            if(a[i]<min)
                min=a[i];
        }
        System.out.println(min);
    }
}
